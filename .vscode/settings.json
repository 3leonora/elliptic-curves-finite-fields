{
    "C_Cpp.clang_format_fallbackStyle": "Microsoft",
    "C_Cpp.clang_format_style": "file",
    "files.associations": {
        "exception": "cpp",
        "format": "cpp",
        "numbers": "cpp"
    },
    "C_Cpp.default.cppStandard": "c++20",
    "C_Cpp.default.includePath": [
        "${workspaceFolder}/src/libs"
    ],
    "cSpell.words": [
        "bazel",
        "bazelrc",
        "crosstool",
        "cxxopt",
        "hexflg",
        "maxuint",
        "pipow",
        "primefield",
        "primegaps",
        "uinvert",
        "ulll"
    ]
}